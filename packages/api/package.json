{
  "name": "@joyutils/transcoder-api",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "db:generate": "bunx drizzle-kit generate --dialect sqlite --schema ./src/db/schema.ts",
    "db:migrate": "bun run migrate.ts",
    "db:reset": "rimraf drizzle && rimraf data/sqlite.db",
    "db:fullreset": "bun run db:reset && bun run db:generate && bun run db:migrate",
    "uploads:clean": "rimraf uploads",
    "codegen": "graphql-codegen",
    "dev": "bun run src/index.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@types/bun": "latest",
    "drizzle-kit": "^0.25.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@joystream/metadata-protobuf": "^2.16.0",
    "@joystream/types": "^4.6.0",
    "blake3": "2.1.7",
    "drizzle-orm": "^0.34.1",
    "file-type": "^18.2.1",
    "graphql": "^16.9.0",
    "graphql-request": "5",
    "multihashes": "^4.0.3",
    "rimraf": "^6.0.1"
  }
}
